import java.util.Scanner;
class Main
{
    	public static void main(String args[])
        {
            // fill your code
          Scanner sa = new Scanner(System.in);	
          int a =sa.nextInt();
          int b =sa.nextInt();
		  int sum= a+b;
          int diff=a-b;
          System.out.println(sum);
          System.out.print(diff);
        }
}
